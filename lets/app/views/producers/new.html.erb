<% provide :title, "New Producer"%>
<h3>Producers#new</h3>
<%= form_for(@producer) do |f| %>
 	<% @object_with_errors = @producer %>
	<%= render 'shared/error_messages' %>

  <%= f.label :name %>
  <%= f.text_field :name, class: 'form-control' %>

  <%= f.label :fantasy_name %>
  <%= f.text_field :fantasy_name, class: 'form-control' %>

  <%= f.label :social_id %>
  <%= f.text_field :social_id, class: 'form-control' %>

  <%= f.submit "Create", class: "btn btn-primary" %>
<% end %>
