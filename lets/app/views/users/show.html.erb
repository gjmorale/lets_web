<% provide(:title, @user.first_name) %>
<h1>
<%= @user.first_name %> <%= @user.last_name %>
</h1>
<h2><%= @user.social_id %></h2>
<p><%= @user.public_gender %></p>
<p><%= @user.birth_date %></p>

<h2>Producers</h2>
<ul>
	<%= render @user.account.producers %>
</ul>

<h2>Purchases</h2>
<ul>
	<%= render @user.purchases %>
</ul>
 